{
    "manifest_version": 3,
    "name": "OGE Parser",
    "version": "5.7",
    "description": "Une extension pour calculer vos moyennes sur le bilan des notes de OGE",
    "icons": {
        "16": "OGE-Parser-Logo.png",
        "48": "OGE-Parser-Logo.png",
        "128": "OGE-Parser-Logo.png"
    },
  "browser_specific_settings": {
    "gecko": {
      "id": "accortiogo@gmail.com",
      "strict_min_version": "109.0"
    }
  },
    "content_scripts": [
        {
            "matches": [ "https://iutdijon.u-bourgogne.fr/oge/stylesheets/etu/bilanEtu.xhtml" ],
            "js": [ "content.js" ],
            "css": [ "gradesStyle.css" ]
        }
    ]
}